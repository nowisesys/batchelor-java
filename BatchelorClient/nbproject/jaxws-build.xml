<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:j2seproject3="http://www.netbeans.org/ns/j2se-project/3" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${libs.jaxws21.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-BatchelorSoapService" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/BatchelorSoapService"/>
        <property name="wsdl-BatchelorSoapService" location="xml-resources/web-service-references/BatchelorSoapService/wsdl/localhost_8080/BatchelorService/BatchelorSoapService.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/BatchelorSoapService" destdir="${build.generated.dir}/jax-wsCache/BatchelorSoapService" package="se.uu.bmc.it.batchelor.soap.schema" wsdl="${wsdl-BatchelorSoapService}" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="http://localhost:8080/BatchelorService/BatchelorSoapService?wsdl" xnocompile="true">
            <depends file="${wsdl-BatchelorSoapService}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/BatchelorSoapService"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/BatchelorSoapService">
                <include name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="wsimport-client-clean-BatchelorSoapService" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/BatchelorSoapService"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/se/uu/bmc/it/batchelor/soap/schema"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-BatchelorSoapService"/>
</project>
