<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:j2seproject3="http://www.netbeans.org/ns/j2se-project/3" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/client"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${libs.jaxws21.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-BatchelorSoapService" depends="wsimport-init">
        <property name="wsdl-BatchelorSoapService" location="xml-resources/web-service-references/BatchelorSoapService/wsdl/localhost_8080/BatchelorService/BatchelorSoapService.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/client" destdir="${build.generated.dir}/wsimport/client" package="se.uu.bmc.it.batchelor.soap.schema" wsdl="${wsdl-BatchelorSoapService}" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="http://localhost:8080/BatchelorService/BatchelorSoapService?wsdl" xnocompile="true">
            <produces dir="${build.generated.dir}/wsimport/client/se/uu/bmc/it/batchelor/soap/schema" includes="BatchelorSoapService.java" casesensitive="no"/>
        </wsimport>
    </target>
    <target name="wsimport-client-clean-BatchelorSoapService" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/client/se/uu/bmc/it/batchelor/soap/schema"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-BatchelorSoapService"/>
    <target name="wsimport-client-compile-depend" if="do.depend.true">
        <j2seproject3:depend srcdir="${build.generated.dir}/wsimport/client" classpath="${libs.jaxws21.classpath}:${javac.classpath}" destdir="${build.classes.dir}"/>
    </target>
    <target name="wsimport-client-compile" depends="-pre-pre-compile, wsimport-client-compile-depend">
        <j2seproject3:javac srcdir="${build.generated.dir}/wsimport/client" classpath="${libs.jaxws21.classpath}:${javac.classpath}" destdir="${build.classes.dir}"/>
        <copy todir="${build.classes.dir}">
            <fileset dir="${build.generated.dir}/wsimport/client" includes="**/*.xml"/>
        </copy>
    </target>
</project>
